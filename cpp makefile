# Target executable name
TARGET = part_III.out

# Source files
SRCS = main.c code.cpp

# Compiler and flags
CC = g++
# CFLAGS = place flags if needed

# Default target builds the executable
$(TARGET): $(SRCS)
	$(CC) $(SRCS) $(CFLAGS) -o $@

# Clean target removes the executable
clean:
	rm -f $(TARGET)

